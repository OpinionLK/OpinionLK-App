{
  "expo": {
    "name": "OpinionLK-App",
    "slug": "OpinionLK-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Logosplash.png",
      "width": "200",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-foreground.png",
        "backgroundColor": "#C0CAF9"
      }
    },
    "web": {
      "favicon": "./assets/app-foreground.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
